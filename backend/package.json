{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for foodplan app",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-dev-node": "nodemon ./src/app.ts",
    "start-dev-ng": "cd ../frontend/ && ng build --watch",
    "start-dev-database": "cd ../database && nodemon ../src/index.ts",
    "start-dev-all": "concurrently \"npm run start-dev-node\" \"npm run start-dev-ng\" \"npm run start-dev-database\" -n node,ts,ng -c yellow,blue,red -k",
    "dev:debug": "xcopy node_modules ..\\build\\backend\\dist\\node_modules /s /e /I /y && concurrently \"tsc --watch\" \"nodemon --inspect ../build/backend/dist/app.js\" \"cd ../frontend && ng build --watch\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "tslint": "^5.12.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-openid-connect": "^2.5.1",
    "got": "^11.8.2",
    "jquery": "^3.6.0",
    "jwks-rsa": "^2.0.5",
    "mariadb": "^2.5.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tslog": "^3.2.2"
  }
}